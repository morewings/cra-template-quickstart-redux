{
  "name": "cra-template-quickstart-redux",
  "version": "0.4.1",
  "license": "MIT",
  "engines": {
    "node": ">=10"
  },
  "description": "Opinionated quickstart Create React App template with Redux, enzyme and custom eslint configuration",
  "main": "template.json",
  "keywords": [
    "react",
    "create-react-app",
    "cra-template",
    "template",
    "enzyme",
    "eslint",
    "redux"
  ],
  "files": [
    "template",
    "template.json"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/morewings/cra-template-quickstart-redux",
    "directory": "/"
  },
  "bugs": {
    "url": "https://github.com/morewings/cra-template-quickstart-redux/issues"
  },
  "dependencies": {
    "enzyme": "^3.11.0",
    "enzyme-adapter-react-16": "^1.15.2",
    "enzyme-to-json": "^3.4.3",
    "eslint-config-airbnb": "^18.0.1",
    "eslint-config-prettier": "^6.9.0",
    "eslint-plugin-fp": "^2.3.0",
    "eslint-plugin-prettier": "^3.1.2",
    "husky": "4.2.3",
    "lint-staged": "^10.0.0",
    "node-sass": "^4.13.0",
    "prettier": "^1.19.1",
    "prop-types": "^15.7.2",
    "react": "^16.12.0",
    "react-dom": "^16.12.0",
    "react-redux": "^7.1.3",
    "react-scripts": "3.4.0",
    "redux": "^4.0.5",
    "redux-mock-store": "^1.5.4"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "clean-files": "rm -rf ./template/public ./template/src && rm -f ./template/.env ./template/.eslintrc  ./template/.nvmrc ./template/.prettierrc ./template/jsconfig.json ./template/.huskyrc ./template/.lintstagedrc ./template/README.md ./template/README_CRA.md",
    "copy-files": "cp -a ./src/. template/src && cp -a ./public/. template/public && cp .env .eslintrc .nvmrc .prettierrc jsconfig.json .huskyrc .lintstagedrc README.md README_CRA.md template/",
    "prepublishOnly": "yarn clean-files && yarn copy-files",
    "fix": "eslint --fix ./src/",
    "lint": "eslint ./src/"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "jest": {
    "snapshotSerializers": [
      "enzyme-to-json/serializer"
    ]
  }
}
