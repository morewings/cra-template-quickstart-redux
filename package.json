{
  "name": "cra-template-quickstart-redux",
  "version": "0.5.1",
  "license": "MIT",
  "engines": {
    "node": ">=10"
  },
  "description": "Opinionated quickstart Create React App template with Redux, enzyme and custom eslint configuration",
  "main": "template.json",
  "keywords": [
    "react",
    "create-react-app",
    "cra-template",
    "template",
    "enzyme",
    "eslint",
    "redux"
  ],
  "files": [
    "template",
    "template.json"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/morewings/cra-template-quickstart-redux",
    "directory": "/"
  },
  "bugs": {
    "url": "https://github.com/morewings/cra-template-quickstart-redux/issues"
  },
  "dependencies": {
    "@testing-library/jest-dom": "^5.3.0",
    "@testing-library/react": "^10.0.1",
    "@testing-library/react-hooks": "^3.2.1",
    "@testing-library/user-event": "^10.0.0",
    "axios": "^0.19.2",
    "eslint-config-airbnb": "^18.1.0",
    "eslint-config-prettier": "^6.10.1",
    "eslint-plugin-fp": "^2.3.0",
    "eslint-plugin-prettier": "^3.1.2",
    "husky": "^4.2.3",
    "lint-staged": "^10.0.10",
    "prettier": "^2.0.2",
    "prop-types": "^15.7.2",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-redux": "^7.2.0",
    "react-scripts": "3.4.1",
    "react-test-renderer": "^16.13.1",
    "redux": "^4.0.5",
    "redux-mock-store": "^1.5.4",
    "redux-promise-middleware": "^6.1.2",
    "stylelint": "13.2.1",
    "stylelint-config-standard": "20.0.0",
    "stylelint-order": "4.0.0"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "clean:files": "rm -rf ./template/public ./template/src && rm -f ./template/.env ./template/.eslintrc  ./template/.nvmrc ./template/.prettierrc ./template/jsconfig.json ./template/.huskyrc ./template/.lintstagedrc ./template/README.md ./template/README_CRA.md ./template/.stylelintrc.json",
    "copy:files": "cp -a ./src/. template/src && cp -a ./public/. template/public && cp .env .eslintrc .nvmrc .prettierrc jsconfig.json .lintstagedrc README.md README_CRA.md .stylelintrc.json template/ && cp .huskyrc template/huskyrc-template",
    "prepublishOnly": "yarn clean:files && yarn copy:files",
    "fix:js": "npx eslint --fix ./src/",
    "lint:js": "npx eslint ./src/",
    "lint:style": "npx stylelint ./src/",
    "fix:style": "npx stylelint ./src/ --fix"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
