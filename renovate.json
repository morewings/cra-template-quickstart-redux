{
  "extends": [
    "config:base"
  ],
  "packageRules": [
    {
      "paths": ["./package.json", "./template.json"],
      "updateTypes": ["minor", "patch", "pin", "digest"],
      "automerge": true
    }
  ],
  "npm": {
    "fileMatch": ["(^|/)template.json$"]
  },
  "rangeStrategy": "replace",
  "automergeType": "branch"
}
