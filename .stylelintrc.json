{
  "extends": "stylelint-config-standard",
  "rules": {
    "function-calc-no-unspaced-operator": true,
    "order/order": [
      "custom-properties",
      "declarations"
    ],
    "order/properties-alphabetical-order": true
  },
  "plugins": [
    "stylelint-order"
  ],
  "ignoreFiles": ["**/*.snap"]
}